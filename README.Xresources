!k8-yterm.keybind.color-mono.color-mode: bw

!k8-yterm.keybind.color-green.bind: C-H-3
!k8-yterm.keybind.color-green.color-mode: green

k8-yterm.keybind.paste-primary.bind: S-Insert
k8-yterm.keybind.paste-primary.paste: primary
k8-yterm.keybind.paste-secondary.bind: M-Insert
k8-yterm.keybind.paste-secondary.paste: secondary
k8-yterm.keybind.paste-clipboard.bind: M-S-Insert
k8-yterm.keybind.paste-clipboard.paste: clipboard

k8-yterm.keybind.copy-to-primary.bind: S-Insert
k8-yterm.keybind.copy-to-primary.copy-to: primary
k8-yterm.keybind.copy-to-secondary.bind: M-Insert
k8-yterm.keybind.copy-to-secondary.copy-to: secondary
k8-yterm.keybind.copy-to-clipboard.bind: M-S-Insert
k8-yterm.keybind.copy-to-clipboard.copy-to: clipboard
